{
    "name": "danielkulbe/i18n",
    "description": "Internationalisation for bolt",
    "type": "bolt-extension",
    "keywords": [
        "i18n", "internationalisation", "localisation", "locale", "translate", "multilingual"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Kulbe",
            "email": "info@daniel-kulbe.de"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "http://gitlab.daniel-kulbe.de/root/bolt-internationalisation.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:DanielKulbe/Internationalisation.git"
        }
    ],
    "require": {
        "bolt/bolt": ">=2.2.0,<3.0.0"
    },
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\DanielKulbe\\Internationalisation\\": ""
        }
    },
    "extra": {
        "bolt-assets": "assets",
        "bolt-icon": "assets/i18n-symbol.png",
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    }
}